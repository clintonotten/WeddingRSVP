# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'WeddingRSVP' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for WeddingRSVP
  	pod ‘SwiftyJSON’, ‘3.0.0’
  	pod ‘Alamofire’, ‘~> 4.0’
  	pod 'Firebase/Core' #Default Firebase functionality
	pod 'Firebase/Database' #Real time database
	pod 'Firebase/Auth'     #Firebase Authentication
	pod 'Firebase/Storage'  #Firebase Storage
end
